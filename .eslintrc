{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": [
    ],
    "rules": {
      "vue/multi-word-component-names": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "vue/no-multiple-template-root": "off",
      "vue/comment-directive": 0,
    },
  }